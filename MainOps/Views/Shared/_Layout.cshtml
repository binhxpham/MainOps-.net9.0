                            $("#DownloadsContainer").toggle()
                        }
                        else {

                        }
                    }
                });
                </text>
            }

            document.addEventListener('click', function (event) {
                    var dropdownmenu = document.getElementById("thedropdownmenu")
                if (!dropdownmenu.contains(event.target)) {
                    if (!dropdownmenu.classList.contains('collapsing')) {
                            if (dropdownmenu.style.height == "0.993217px") {
                                dropdownmenu.style.height = "auto";
                            }
                            else {
                                var elements = document.getElementsByClassName("dropdowndropdown-content");
                                for (var i = 0; i < elements.length; i++) {
                                    if (elements[i].style.display == "block") {
                                        elements[i].style.display = "none";
                                    }
                                }
                                dropdownmenu.style.height = "0.993217px";
                                dropdownmenu.classList.remove('in');
                                dropdownmenu.classList.add('collapse');
                            }
                        }

                    }
            });
            var elements = document.getElementsByClassName("dropdowndropdown");
            var myFunction = function () {
                var element = this.getElementsByClassName("dropdowndropdown-content")[0];
                if (element.style.display == "block") {
                    element.style.display = "none";
                }
                else {
                    element.style.display = "block";
                }


            };
            for (var i = 0; i < elements.length; i++) {
                elements[i].addEventListener('click', myFunction, false);
            }


        });
