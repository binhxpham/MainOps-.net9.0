                                                    <a asp-area="" asp-controller="StockRentalItems" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Rental Administration")</a>
                                                    <a asp-area="" asp-controller="Item_Location" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Internal Rent")</a>
