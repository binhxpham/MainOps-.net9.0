                                                    <a asp-controller="WellDrilling" asp-action="Instructions">@SharedLocalizer.GetLocalizedHtmlString("Drilling Instructions")</a>
                                                    @if (User.IsInRole("Manager") || User.IsInRole("Admin") || User.IsInRole("DivisionAdmin"))
