                                {
                                    if (user.DivisionId != 10)
                                    {
                                        <li class="dropdowndropdown">
                                            <button class="dropdowndropbtn">
                                                @SharedLocalizer.GetLocalizedHtmlString("Project Actions")
                                                <span class="dropdowndropdown-content">
                                                    @if (User.IsInRole("ExternalDriller"))
                                                    {
                                                        <a asp-controller="TrackItems" asp-action="Daily_Report">@SharedLocalizer.GetLocalizedHtmlString("DAILY REPORT - Direct Link")</a>
                                                        <a asp-area="" asp-controller="TrackItems" asp-action="DailyReports">@SharedLocalizer.GetLocalizedHtmlString("Daily Reports")</a>
                                                    }

                                                    @if (User.IsInRole("Admin") || User.IsInRole("DivisionAdmin") || User.IsInRole("Manager") || User.IsInRole("AlarmTeam"))
                                                    {
                                                        <a asp-area="" asp-controller="Projects" asp-action="AlertCallProjects">@SharedLocalizer.GetLocalizedHtmlString("Alert Call Projects")</a>
