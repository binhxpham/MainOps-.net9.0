                                                {
                                                    <a asp-area="" asp-controller="ProjectStatus" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Tender List")</a>
                                                }
                                                else
                                                {
                                                    <a asp-area="" asp-controller="Projects" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Projects")</a>
                                                    <a asp-area="" asp-controller="SubProjects" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("SubProjects")</a>

                                                    @if (User.IsInRole("Admin") || User.IsInRole("DivisionAdmin"))
                                                    {
                                                        if (user.DivisionId != 10)
                                                        {
                                                            <a asp-area="" asp-controller="ProjectUsers" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Project Users")</a>
                                                            <a asp-area="" asp-controller="AlarmReportReceivers" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Alarm Report Receivers")</a>
