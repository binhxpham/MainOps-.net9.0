                                                <a asp-area="" asp-controller="Meas" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Measurements")</a>
                                                <a asp-area="" asp-controller="MeasPoints" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Measurement Points")</a>
                                                <a asp-area="" asp-controller="MeasPoints" asp-action="ExportDipList">@SharedLocalizer.GetLocalizedHtmlString("Dips")</a>
                                                <a asp-area="" asp-controller="Offsets" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Offsets")</a>
                                                @if (User.IsInRole("Admin") || user.DivisionId.Equals(9))
                                                {
                                                    <a asp-area="" asp-controller="MeasPoints" asp-action="ReUploadToFTP">@SharedLocalizer.GetLocalizedHtmlString("ReUpload to FTP")</a>
                                                }
                                                @if (!User.IsInRole("Guest"))
                                                {
                                                    <a asp-area="" asp-controller="Documentation" asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Documentation")</a>
                                                }
                                            </span>
                                        </button>
                                    </li>
                                }
                                @if (User.IsInRole("Admin"))
                                {
                                    if (user.DivisionId != 10)
                                    {
                                        <li class="dropdowndropdown">
                                            <button class="dropdowndropbtn">
                                                @SharedLocalizer.GetLocalizedHtmlString("Super Admin Page")
