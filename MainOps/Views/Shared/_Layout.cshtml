<body>

    <!--The id topnavbar is needed, that the navbar fixed top isn't overlapping the content of the page-->

    <nav class="navbar navbar-default navbar-fixed-top" id="topnavbar" style="margin-bottom:0px;">
        @if (SignInManager.IsSignedIn(User))
        {
            <span id="topbarshowbutton" hidden="hidden" onclick="showthetop();" style="cursor:pointer;">X</span>
            <table id="TheTopTable" style="table-layout:fixed;">
                @*<tr id="topbarshowbutton" hidden="hidden" style="cursor:pointer;">
                        <td></td>
                    </tr>*@
                <tr>
                    <td><span onclick="hidethetop();" style="cursor:pointer;">X</span></td>
                    <td>@await Html.PartialAsync("_SelectLanguagePartial")</td>
                    <td>
                        <form asp-area="" asp-controller="Account" asp-action="Logout" method="post" id="logoutForm">
                            <button type="submit" class="btn-hoelscher">@SharedLocalizer.GetLocalizedHtmlString("Log out")</button>
                        </form>
                    </td>
                    <td>
                        <form asp-area="" asp-controller="Manage" asp-action="Index" method="get" title="Manage">
                            <button class="btn-hoelscher">@SharedLocalizer.GetLocalizedHtmlString("Account")</button>
                        </form>
                    </td>
                    @if (User.IsInRole("AlarmTeam") || User.IsInRole("Admin"))
                    {
                        <td>
                            <form asp-area="" asp-controller="TrackItems" asp-action="SystemCheck" method="post">
                                <button class="btn-hoelscher">@SharedLocalizer.GetLocalizedHtmlString("System Checked!")</button>
                            </form>
                        </td>
                    }
                    <td id="TasksContainer" hidden="hidden"><button class="blinking btn-hoelscher" onclick="GetUserTasks();">@SharedLocalizer.GetLocalizedHtmlString("Tasks") (<span id="NumberOfTasks" style="color:red;font:bold;">0</span>)!</button></td>
                    <td id="DownloadsContainer" hidden="hidden"><button class="blinking btn-hoelscher" onclick="GetUserDownloads();">@SharedLocalizer.GetLocalizedHtmlString("Downloads") (<span id="NumberOfDownloads" style="color:red;font:bold;">0</span>)!</button></td>
                </tr>
            </table>
        }
        else
        {
            <div class="navbar-header" id="topheader" style="width:100%;background-color:white;border:none;">
                <table style="width:100%;">
                    <tr>
                        <td style="width:10%;">
                            @await Html.PartialAsync("_LoginPartial")
                        </td>
                        <td style="width:70%;">
                            @await Html.PartialAsync("_SelectLanguagePartial")
                        </td>
                        <td id="logopart" style="width:80%;text-align:right;">
                            @if (theculture == "nl-NL")
                            {
                                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand"><img src="~/images/Logo Tjaden RGB new.png" alt="Hölscher Jensen A/S" id="logo" width="333" height="146" /></a>
                            }
                            else
                            {
                                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand"><img src="~/images/Logo-holscher-jensen_fritlagt_i_lille.png" alt="Hölscher Jensen A/S" id="logo" /></a>
                            }

                        </td>
                    </tr>
                </table>
            </div>
        }

        <div class="navbar-header" style="width:100%;height:50px;overflow:visible !important;border:none;">
