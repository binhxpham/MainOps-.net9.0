@model MainOps.Models.CGJClasses.Building

@{
    ViewData["Title"] = "Opret_Bygning";
}

<h2>Opret bygning</h2>

<h4>Bygning</h4>
<hr />
<div class="row">
    <div class="col-xs-12">
        <form asp-action="Create_Building">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input asp-for="path" hidden="hidden" />
            <div class="form-group">
                <label asp-for="ProtocolId" class="control-label"></label>
                <select asp-for="ProtocolId" class ="form-control" asp-items="ViewBag.ProtocolId"></select>
            </div>
            <div class="form-group">
                <label asp-for="Location" class="control-label"></label>
                <input asp-for="Location" class="form-control" />
                <span asp-validation-for="Location" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Address" class="control-label"></label>
                <input asp-for="Address" class="form-control" />
                <span asp-validation-for="Address" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Opret" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="CGJensenMainMenu">Tilbage</a>
</div>

