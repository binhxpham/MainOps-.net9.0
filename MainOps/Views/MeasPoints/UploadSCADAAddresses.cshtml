@{
    ViewData["Title"] = SharedLocalizer.GetLocalizedHtmlString("Upload SCADA Addresses (2 column CSV file Add,Name");
}

<h2>
    Upload SCADA ADDRESSES
</h2>

<form asp-action="UploadSCADAAddresses" asp-controller="MeasPoints" method="post" enctype="multipart/form-data">
    <input type="file" accept=".csv" name="postedFile" />
    <button class="btn-hoelscher">@SharedLocalizer.GetLocalizedHtmlString("Upload")</button>
</form>
