@model MainOps.Models.Logger


<h2>@SharedLocalizer.GetLocalizedHtmlString("Create")</h2>

<h4>@SharedLocalizer.GetLocalizedHtmlString("Logger")</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="LoggerNo" class="control-label"></label>
                <input asp-for="LoggerNo" class="form-control" />
                <span asp-validation-for="LoggerNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SerialNo" class="control-label"></label>
                <input asp-for="SerialNo" class="form-control" />
                <span asp-validation-for="SerialNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SimCardNo" class="control-label"></label>
                <input asp-for="SimCardNo" class="form-control" />
                <span asp-validation-for="SimCardNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="@SharedLocalizer.GetLocalizedHtmlString("Create")" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Back to List")</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
