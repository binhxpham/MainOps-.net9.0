@model MainOps.Models.WaterSamplePlace

@{
    ViewData["Title"] = "WaterSamplePlace";
}

<h2>WaterSamplePlace</h2>

<h4>WaterSamplePlace</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="WaterSamplePlace">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProjectId" class="control-label"></label>
                <select asp-for="ProjectId" class="form-control" asp-items="ViewBag.ProjectId"></select>
            </div>
            <div class="form-group">
                <label asp-for="WaterSamplePlaceTypeId" class="control-label"></label>
                <select asp-for="WaterSamplePlaceTypeId" class="form-control" asp-items="ViewBag.WaterSamplePlaceTypeId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <p><a asp-action="Index">@SharedLocalizer.GetLocalizedHtmlString("Back to Main WaterSample Page")</a></p>
</div>

